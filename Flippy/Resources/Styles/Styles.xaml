<?xml version="1.0" encoding="UTF-8" ?>
<?xaml-comp compile="true" ?>
<ResourceDictionary 
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml">

    <Style TargetType="Label" x:Key="BaseLabel">
        <Setter Property="FontFamily" Value="LatoRegular" />
        
    </Style>

    <Style x:Key="DarkLabel" BasedOn="{StaticResource BaseLabel}" TargetType="Label">
        <Setter Property="TextColor" Value="{StaticResource DarkBlue}" />
    </Style>

    <Style x:Key="GreyLabel" BasedOn="{StaticResource BaseLabel}" TargetType="Label">
        <Setter Property="TextColor" Value="{StaticResource DarkGrey}" />
    </Style>

</ResourceDictionary>
